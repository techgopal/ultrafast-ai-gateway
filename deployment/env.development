# Development Environment Configuration
# Copy this file to .env and modify as needed

# Development mode
DEVELOPMENT_MODE=true

# Logging
RUST_LOG=info
RUST_BACKTRACE=1

# Redis configuration
REDIS_URL=redis://redis:6379

# Gateway JWT Secret (change in production)
GATEWAY_JWT_SECRET=ultrafast-gateway-dev-secret-key

# API Keys (leave empty for development)
OPENAI_API_KEY=
ANTHROPIC_API_KEY=
AZURE_OPENAI_API_KEY=
GOOGLE_VERTEX_AI_API_KEY=
COHERE_API_KEY=
GROQ_API_KEY=
MISTRAL_API_KEY=
PERPLEXITY_API_KEY=

# Gateway API Keys (JSON array format)
GATEWAY_API_KEYS=[{"key":"sk-dev-gateway-key","name":"development","enabled":true,"rate_limit":{"requests_per_minute":1000,"requests_per_hour":10000,"tokens_per_minute":100000},"metadata":{}}]
